@model PositionViewModel

@{
  ViewData["Title"] = "D2CFL.Admin";
}

<h2>Are you realy want to delete @Model.Name</h2>

<div class="btn-group btn-group-justified">
  <form asp-action="Delete" asp-controller="Position" method="post" class="btn-group">
    <input type="submit" value="Delete" class="btn btn-default" />
  </form>
  <form asp-action="Info/@Model.Id" asp-controller="Position" class="btn-group">
    <input type="submit" value="Back" class="btn btn-default" />
  </form>
</div>