@model TeamViewModel

@{
  ViewData["Title"] = "D2CFL.Admin";
}

<h2>@Model.Name</h2>

<div class="container">
  <dl class="dl-horizontal">
    <dt>Name</dt>
    <dd>@Model.Name</dd>
    <dt>Region</dt>
    <dd>@Model.Region</dd>
  </dl>
  <a asp-action="Update" asp-controller="Team" class="btn btn-default">Update</a>
  <a asp-action="Ddlete" asp-controller="Team" class="btn btn-default">Delete</a>
  <a asp-action="Index" asp-controller="Team" class="btn btn-default">Back</a>
</div>